{
  "name": "auto-sui-skills",
  "version": "0.1.0",
  "description": "Auto-generate Claude skills from Sui Move contracts",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "auto-sui-skills": "./dist/bin/auto-sui-skills.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "cli": "node --loader ts-node/esm bin/auto-sui-skills.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "sui",
    "move",
    "claude",
    "skill",
    "abi",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.16.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "handlebars": "^4.7.8",
    "ora": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
